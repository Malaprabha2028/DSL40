#include <iostream>
using namespace std;

int main() 
{
    int choice;
    int r, c;
    int A[10][10], B[10][10], C[10][10];

    cout << "Enter number of rows and columns: ";
    cin >> r >> c; 

    cout << "Enter elements of Matrix A:\n";
    for (int i = 0; i < r; i++) {
        for (int j = 0; j < c; j++) {
            cin >> A[i][j];
        }
    }

    cout << "Enter elements of Matrix B:\n";
    for (int i = 0; i < r; i++) {
        for (int j = 0; j < c; j++) {
            cin >> B[i][j];
        }
    }

    cout << "\nChoose Matrix Operation:\n";
    cout << "1. Addition\n";
    cout << "2. Subtraction\n";
    cout << "3. Multiplication\n";
    cout << "4. Transpose of Matrix A\n";
    cout << "5. Display Matrix A\n";
    cout << "Enter your choice: ";
    cin >> choice;

    switch (ch) 
{

        case 1: // Addition
            for (int i = 0; i < r; i++)
                for (int j = 0; j < c; j++)
                    C[i][j] = A[i][j] + B[i][j];

            cout << "Result of Addition:\n";
            break;

        case 2:
            for (int i = 0; i < r; i++)
                for (int j = 0; j < c; j++)
                    C[i][j] = A[i][j] - B[i][j];

            cout << "Result of Subtraction:\n";
            break;

        case 3: 
            for (int i = 0; i < r; i++) {
                for (int j = 0; j < c; j++) {
                    C[i][j] = 0;
                    for (int k = 0; k < c; k++) {
                        C[i][j] += A[i][k] * B[k][j];
                    }
                }
            }
            cout << "Result of Multiplication:\n";
            break;

        case 4: 
            cout << "Transpose of Matrix A:\n";
            for (int i = 0; i < c; i++) {
                for (int j = 0; j < r; j++) {
                    cout << A[j][i] << " ";
                }
                cout << endl;
            }
            return 0;

        case 5: 
            cout << "Matrix A:\n";
            for (int i = 0; i < r; i++) {
                for (int j = 0; j < c; j++) {
                    cout << A[i][j] << " ";
                }
                cout << endl;
            }
            return 0;

        default:
            cout << "Invalid Choice!";
            return 0;
    }
    for (int i = 0; i < r; i++) {
        for (int j = 0; j < c; j++) {
            cout << C[i][j] << " ";
        }
        cout << endl;
    }

    return 0;
}
